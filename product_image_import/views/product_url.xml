<odoo>

    <record id="product_url_form_edit" model="ir.ui.view">
        <field name="name">Product URL update</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">

            <xpath expr="//form//sheet" position="before">
            <header>

               <button name="import_images" type="object" string="Import Images"
                            class="oe_highlight"/>
            </header>
            </xpath>

        </field>
    </record>



</odoo>