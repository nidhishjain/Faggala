<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="website_customer_wallet_acquirer_form" model="ir.ui.view">
        <field name="name">payment.acquirer.form.wallet</field>
        <field name="model">payment.acquirer</field>
        <field name="inherit_id" ref="payment.acquirer_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='state']" position="after">
                <field name="wallet_acquirer" groups="base.group_no_one"/>
            </xpath>
        </field>
    </record>
</odoo>
